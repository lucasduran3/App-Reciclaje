    "clean": "rm -rf node_modules client/node_modules server/node_modules",
    "reset-data": "node ../scripts/reset-data.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasduran3/App-Reciclaje.git"
  },
  "keywords": [
    "reciclaje",
    "gamificacion",
    "urbano"
  ],

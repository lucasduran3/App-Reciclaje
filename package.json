{
  "name": "app-reciclaje",
  "version": "1.0.0",
  "description": "App gamificada de reciclaje y limpieza urbana",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
